{
  "version": 2,
  "name": "watt-events",
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm run install",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}